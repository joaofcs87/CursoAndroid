package br.senac.pi;

public class EstruturaDeControleFor {

	public static void main(String[] args) {
		for(int i = 1; i <= 100; i++) {  
	          if(i%3 == 0){  
	       System.out.println("Eh Multiplo= " +i);  
	       System.out.println("Nao eh multiplo= " + (i%3 != 0));  
		}

	}

}
